generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
}



model RuasJalan {
  id                Int       @id @default(autoincrement())
  id_ruas_jalan     String    @unique
  nama_ruas_jalan   String
  panjang_km        Float
  sta_awal         String
  sta_akhir        String
  lat_awal    Float
  long_awal   Float
  lat_akhir   Float
  long_akhir  Float
  uptd_id         Int
  uptd            Uptd      @relation(fields: [uptd_id], references: [id])
  sup_id          Int?
  sup             Sup?      @relation(fields: [sup_id], references: [id])
  createdAt        DateTime  @default(now())
  updatedAt        DateTime  @default(now()) @updatedAt
}


model Sup {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  uptd_id   Int
  uptd      Uptd     @relation(fields: [uptd_id], references: [id])
  RuasJalan RuasJalan[]
}

model Uptd {
  id          Int         @id @default(autoincrement())
  name        String      @unique
  alamat      String?     
  lat         Float?
  long        Float?
  RuasJalan   RuasJalan[]
  Sup         Sup[]
}